    "import requests, json\n",
    "\n",
    "auth_code = ''\n",
    "\n",
    "res = requests.get(\n",
    "    url = f\"https://opendata.cwa.gov.tw/fileapi/v1/opendataapi/F-D0047-091?Authorization=CWA-CF82C53C-E945-48C6-8C94-936644153812&downloadType=WEB&format=JSON\"\n",
    ")\n",
    "\n",
    "res.json\n",
    "\n",
    "obj = res.json()\n",
    "\n",
    "for objLoc in obj[\"cwaopendata\"][\"dataset\"][\"locations\"][\"location\"]:\n",
    "   \n",
    "    print(f\"=== 縣市名稱: {objLoc['locationName']}, lat: {objLoc['lat']}, lon: {objLoc['lon']} ===\")\n",
    "    \n",
    "    for we in objLoc[\"weatherElement\"]:\n",
    "\n",
    "        if we['elementName'] == 'T':\n",
    "            print(f\"{we['description']}\")\n",
    "            for t in we[\"time\"]:\n",
    "                print(f\"開始時間: {t['startTime']}, 結束時間: {t['endTime']}\")\n",
    "                print(f\"溫度: {t['elementValue']['measures']} {t['elementValue']['value']}\")"
   ]
